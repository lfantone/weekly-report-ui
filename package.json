{
  "name": "weekly-report-ui",
  "description": "Weekly Report UI",
  "license": "ISC",
  "version": "1.0.0",
  "readme": "https://github.com/lfantone/weekly-report-ui/blob/master/README.md",
  "repository": "lfantone/weekly-report-ui",
  "nativescript": {
    "id": "org.nativescript.weekly",
    "tns-android": {
      "version": "2.0.0"
    },
    "tns-ios": {
      "version": "2.0.0"
    }
  },
  "scripts": {
    "build.android": "tns build android --release --key-store-path my-release-key.keystore --key-store-password Chapelco123+ --key-store-alias mykey --key-store-alias-password Chapelco123+ && cp platforms/android/build/outputs/apk/weeklyreportui-release.apk ~/Desktop",
    "run.android": "tns run android --emulator",
    "run.ios": "tns run ios --emulator",
    "sync.android": "tns livesync android --emulator --watch",
    "sync.ios": "tns livesync ios --emulator --watch",
    "clean": "rm -rf platforms/android/build"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "lodash.bind": "^4.1.4",
    "lodash.get": "^4.3.0",
    "nativescript-angular": "0.1.2",
    "nativescript-effects": "^0.3.0",
    "rxjs": "^5.0.0-beta.6",
    "tns-core-modules": "^2.0.0"
  },
  "devDependencies": {
    "babel-traverse": "6.8.0",
    "babel-types": "6.8.1",
    "babylon": "6.8.0",
    "codelyzer": "0.0.19",
    "filewalker": "0.1.2",
    "lazy": "1.0.11",
    "nativescript-dev-typescript": "^0.3.2",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10"
  }
}
